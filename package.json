{
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ssl": "sudo ng serve --ssl true --host desktop.homevpn.org --port 443",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "kill_port": "sudo kill $(sudo lsof -t -i:443)",
    "build_docs": "ng build --output-path docs --base-href https://desktop.homevpn.org/ && cp ./CNAME ./docs/CNAME && cp ./docs/index.html ./docs/404.html"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.8",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@datorama/akita": "^8.0.1",
    "ng-let": "^15.0.2",
    "rxjs": "~7.8.0",
    "ts-deep-equal": "^1.1.5",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.1",
    "@angular/cli": "~15.2.1",
    "@angular/compiler-cli": "^15.2.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4"
  }
